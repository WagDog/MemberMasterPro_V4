<p id="notice"><%= notice %></p>

<h1>Listing Plus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price 1</th>
      <th>Price 2</th>
      <th>Price 3</th>
      <th>Price 4</th>
      <th>Price 5</th>
      <th>Price 6</th>
      <th>Group</th>
      <th>Plu cost</th>
      <th>Vat</th>
      <th>Stock mode</th>
      <th>Rp sort group</th>
      <th>Purse no</th>
      <th>Kp printer</th>
      <th>Barcode</th>
      <th>Price type</th>
      <th>Condiment</th>
      <th>Modifiers</th>
      <th>Popup window 1</th>
      <th>Popup window 2</th>
      <th>Popup window 3</th>
      <th>Popup window 4</th>
      <th>Popup window 5</th>
      <th>Back colour</th>
      <th>Font colour</th>
      <th>Is active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plus.each do |plu| %>
      <tr>
        <td><%= plu.name %></td>
        <td><%= plu.price_1 %></td>
        <td><%= plu.price_2 %></td>
        <td><%= plu.price_3 %></td>
        <td><%= plu.price_4 %></td>
        <td><%= plu.price_5 %></td>
        <td><%= plu.price_6 %></td>
        <td><%= plu.group %></td>
        <td><%= plu.plu_cost %></td>
        <td><%= plu.vat %></td>
        <td><%= plu.stock_mode %></td>
        <td><%= plu.rp_sort_group %></td>
        <td><%= plu.purse_no %></td>
        <td><%= plu.kp_printer %></td>
        <td><%= plu.barcode %></td>
        <td><%= plu.price_type %></td>
        <td><%= plu.condiment %></td>
        <td><%= plu.modifiers %></td>
        <td><%= plu.popup_window_1 %></td>
        <td><%= plu.popup_window_2 %></td>
        <td><%= plu.popup_window_3 %></td>
        <td><%= plu.popup_window_4 %></td>
        <td><%= plu.popup_window_5 %></td>
        <td><%= plu.back_colour %></td>
        <td><%= plu.font_colour %></td>
        <td><%= plu.is_active %></td>
        <td><%= link_to 'Show', plu %></td>
        <td><%= link_to 'Edit', edit_plu_path(plu) %></td>
        <td><%= link_to 'Destroy', plu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plu', new_plu_path %>
